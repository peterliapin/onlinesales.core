{
  "AllowedHosts": "*",
  "MigrateOnStart": true,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Postgres": {
    "Server": "localhost",
    "Port": "5432",
    "UserName": "postgres",
    "Password": "postgres",
    "Database": "onlinesales"
  },
  "Elasticsearch": {
    "Server": "localhost",
    "Port": "9200",
    "UseHttps": false,
    "UserName": "elastic",
    "Password": "elastic"
  },
  "SmtpServer": {
    "Server": "smtp.server.com",
    "Port": "25",
    "UserName": "smtp_user",
    "Password": "smtp_password",
    "UseSSL": true
  },
  "TaskRunner": {
    "CronSchedule": "0/5 * * * * ?"
  }
}
