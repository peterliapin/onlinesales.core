{
  "SmsAccessKey": "$SMSACCESSKEY",
  "Gateways": {
    "AmazonSns": {
      "AccessKeyId": "$GATEWAYS__AMAZONSNS__ACCESSKEYID",
      "SecretAccessKey": "$GATEWAYS__AMAZONSNS__SECRETACCESSKEY",
      "DefaultRegion": "$GATEWAYS__AMAZONSNS__DEFAULTREGION"
    },
    "Smsc": {
      "ApiUrl": "https://smsc.ru/sys/send.php",
      "SenderId": "$GATEWAYS__SMSC__SENDERID",
      "Login": "$GATEWAYS__SMSC__LOGIN",
      "Password": "$GATEWAYS__SMSC__PASSWORD"
    },
    "Getshoutout": {
      "ApiUrl": "https://api.getshoutout.com",
      "SenderId": "$GATEWAYS__GETSHOUTOUT__SENDERID",
      "ApiKey": "$GATEWAYS__GETSHOUTOUT_APIKEY"
    }
  },
  "CountryGateways": [
    {
      "Code": "default",
      "Gateway": "AmazonSns"
    },
    {
      "Code": "+94",
      "Gateway": "Getshoutout"
    },
    {
      "Code": "+7",
      "Gateway": "Smsc"
    }
  ]
}